{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "analytics": false,
    "defaultCollection": "@angular-eslint/schematics",
    "cache": {
      "enabled": false
    }
  },
  "newProjectRoot": "projects",
  "projects": {
    "task-ezy": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "allowedCommonJsDependencies": [
              "apexcharts",
              "highlight.js",
              "ng2-pdf-viewer",
              "jwt-decode",
              "file-saver",
              "xlsx",
              "moment-range",
              "jspdf",
              "buffer",
              "js-cookie",
              "zen-observable",
              "axios",
              "fast-xml-parser",
              "uuid",
              "ulid",
              "zen-push",
              "url",
              "isomorphic-unfetch",
              "crypto-js/enc-utf8",
              "crypto-js/hmac-sha256",
              "crypto-js/enc-base64",
              "ng-intercom"
            ],
            "assets": [
              "src/assets",
              "src/favicon-16x16.png",
              "src/favicon-32x32.png",
              {
                "glob": "_redirects",
                "input": "src",
                "output": "/"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/@fuse/styles"
              ]
            },
            "styles": [
              "src/@fuse/styles/tailwind.scss",
              "src/@fuse/styles/themes.scss",
              "src/@fuse/styles/main.scss",
              "node_modules/angular-calendar/scss/angular-calendar.scss",
              "node_modules/@angular/cdk/overlay-prebuilt.css",
              "node_modules/@perfectmemory/ngx-contextmenu/src/assets/stylesheets/base.scss",
              "src/styles/vendors.scss",
              "src/styles/styles.scss",
              "src/styles/taskezy_font_style.css",
              "src/styles/tailwind.scss"
            ],
            "scripts": [],
            "aot": false,
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true
              },
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ]
            },
            "stage": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.stage.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true
              },
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ]
            },
            "test": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ],
              "extractLicenses": false,
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true
              },
              "namedChunks": true,
              "vendorChunk": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "20mb",
                  "maximumError": "20mb"
                }
              ],
              "customWebpackConfig": {
                "path": "./cypress/coverage.webpack.ts"
              }
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ]
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.ts"
                }
              ]
            },
            "e2e": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.e2e.ts"
                }
              ],
              "extractLicenses": false,
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true
              },
              "namedChunks": true,
              "vendorChunk": true,
              "customWebpackConfig": {
                "path": "./cypress/coverage.webpack.ts"
              }
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "task-ezy:build"
          },
          "configurations": {
            "prod": {
              "browserTarget": "task-ezy:build:production"
            },
            "stage": {
              "browserTarget": "task-ezy:build:stage"
            },
            "test": {
              "browserTarget": "task-ezy:build:test"
            },
            "e2e": {
              "browserTarget": "task-ezy:build:e2e"
            },
            "dev": {
              "browserTarget": "task-ezy:build:dev"
            },
            "local": {
              "browserTarget": "task-ezy:build:local"
            }
          }
        },
        "serve-coverage": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "task-ezy:build:e2e"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "task-ezy:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "fileReplacements": [
              {
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.e2e.ts"
              }
            ],
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "./karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon-16x16.png",
              "src/favicon-32x32.png",
              "src/assets"
            ],
            "styles": [
              "src/styles/styles.scss"
            ],
            "scripts": [],
            "codeCoverageExclude": [
              "/**/*.spec.ts",
              "src/@fuse/**/*.*",
              "src/app/fake-db/**/*.*",
              "src/app/mock-api/**/*.*",
              "src/app/environments/**/*.*"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "e2e": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "task-ezy:serve-coverage",
            "watch": true,
            "headless": false
          },
          "configurations": {
            "production": {
              "devServerTarget": "task-ezy:serve-coverage:production"
            }
          }
        },
        "e2e-ci": {
          "builder": "@cypress/schematic:cypress",
          "options": {
            "devServerTarget": "task-ezy:serve-coverage",
            "watch": false,
            "headless": true
          },
          "configurations": {
            "production": {
              "devServerTarget": "task-ezy:serve-coverage:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "task-ezy"
}